{
  "name": "@yarnpkg/pnp",
  "version": "2.1.0",
  "license": "BSD-2-Clause",
  "main": "./sources/index.ts",
  "dependencies": {
    "@types/node": "^13.7.0",
    "@yarnpkg/fslib": "workspace:^2.1.0",
    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@yarnpkg/builder": "workspace:^2.1.0",
    "@yarnpkg/libzip": "workspace:^2.1.0",
    "@yarnpkg/pnpify": "workspace:^2.1.0",
    "typescript": "^3.9.5",
    "webpack": "^5.0.0-beta.17",
    "webpack-cli": "^4.0.0-beta.8",
    "webpack-sources": "^1.4.3"
  },
  "scripts": {
    "build:pnp:hook": "run pnpify webpack-cli --config webpack.config.hook.js",
    "build:pnp": "run pnpify webpack-cli --config webpack.config.pkg.js",
    "postpack": "rm -rf lib",
    "prepack": "run build:compile packages/yarnpkg-pnp --emitDeclarationOnly && run build:pnp",
    "release": "yarn npm publish",
    "update-local": "run build:pnp:hook"
  },
  "publishConfig": {
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts"
  },
  "files": [
    "/lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/yarnpkg/berry.git"
  },
  "engines": {
    "node": ">=10.19.0"
  }
}
