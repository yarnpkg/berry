{
  "name": "@berry/pnpify",
  "version": "0.1.0",
  "main": "./sources/index.ts",
  "bin": "./lib/bin.js",
  "sideEffects": false,
  "dependencies": {
    "@berry/fslib": "workspace:0.1.0",
    "cross-spawn": "^6.0.5"
  },
  "devDependencies": {
    "@berry/builder": "workspace:0.1.0",
    "@berry/pnp": "workspace:0.1.0",
    "@types/eslint": "4.16.6",
    "eslint": "^5.16.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  },
  "peerDependencies": {
    "eslint": "*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "scripts": {
    "build:pnpify": "run webpack-cli --config webpack.config.pkg.js",
    "prepack": "rm -rf lib && run build:pnpify && yarn pnpify tsc --emitDeclarationOnly --declaration",
    "release": "yarn npm publish",
    "test:pnpify:watch": "run test:pnpify --watch",
    "test:pnpify": "run test:unit packages/berry-pnpify"
  },
  "publishConfig": {
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts"
  },
  "files": [
    "/lib/**/*",
    "/bin.js"
  ]
}
