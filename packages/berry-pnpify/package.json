{
  "name": "@berry/pnpify",
  "version": "0.0.2",
  "main": "./sources/index.ts",
  "bin": "./lib/bin.js",
  "sideEffects": false,
  "dependencies": {
    "@berry/fslib": "workspace:0.0.5"
  },
  "devDependencies": {
    "@berry/builder": "workspace:0.0.0",
    "@berry/pnp": "workspace:0.0.2",
    "typescript": "^3.3.3333",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  },
  "scripts": {
    "build:pnpify": "run webpack-cli --config webpack.config.pkg.js && node -r ./lib -r typescript/bin/tsc -- tsc --emitDeclarationOnly --declaration",
    "release": "yarn npm publish",
    "test:pnpify": "run test:unit packages/berry-pnpify",
    "test:watch:pnpify": "run test:unit --watch packages/berry-pnpify"
  },
  "publishConfig": {
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts"
  },
  "files": [
    "/lib/*",
    "/bin.js"
  ]
}
