{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug",
      "runtimeExecutable": "node",
      "runtimeArgs": [
        "--inspect-brk",
        "./scripts/run-yarn.js"
      ],
      "args": [
        "install"
      ],
      "env": {
        "YARN_IGNORE_PATH": "1"
      },
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Single Package Install",
      "runtimeExecutable": "node",
      "cwd": "${workspaceFolder}/../tossme",
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceFolder}/scripts/run-yarn.js"
      ],
      "args": [
        "install"
      ],
      "env": {
        "YARN_IGNORE_PATH": "1"
      },
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outputCapture": "std"
    }
  ]
}