{
  "editor.tabSize": 2,
  "files.eol": "\n",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "npm.packageManager": "yarn",
  "eslint.validate": [
    "javascript",
    "typescript",
    "typescriptreact"
  ],
  "eslint.enable": true,
  "eslint.nodePath": ".yarn/sdks",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  }
}
