{
  "editor.tabSize": 2,
  "files.eol": "\n",
  "typescript.tsdk": ".vscode/pnpify/typescript/lib",
  "eslint.validate": [
    "javascript", // js comment
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
  "eslint.autoFixOnSave": true,
  "eslint.enable": true,
  "eslint.nodePath": ".vscode/pnpify",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}
